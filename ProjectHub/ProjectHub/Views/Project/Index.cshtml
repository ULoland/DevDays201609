@model List<ProjectHub.Models.ProjectModel>

@{
    ViewBag.Title = "Projectlist";
}



<a style="float:right; margin-bottom:15px; padding:20px 35px; background:#eee; color:black;text-decoration:none;" href="/Project/Create">Create new project</a>

<h2>@ViewBag.Title</h2>



@foreach(var project in Model)
{
    
    <p>@project.Name [@project.Id] @Html.ActionLink("Details", "Details", new { id = project.Id })</p>
    
    <hr />
}


